{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "app-api-svc"
    },
    "spec": {
        "selector": {
            "app": "app-api"
        },
        "ports": [
            {
                "name": "api-port",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 8080
            }
        ],
        "type": "NodePort"
    }
}
